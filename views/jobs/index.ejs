<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Company</th>
        <th>Location</th>
    </tr>
  </thead>
  <tbody>
    <% jobs.forEach(function(job) { %>
      <tr>
        <td><%= job.title %></td>
        <td><%= job.description% %></td>
        <td><%= job.company%% %></td>
        <td><%= job.location%% %></td>
        <td><a href="/jobs/<%= job._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>